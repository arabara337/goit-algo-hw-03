from datetime import datetime

def get_days_from_today(date):
    try:
        given_date = datetime.strptime(date, "%Y-%m-%d").date()
        today = datetime.today().date()
        delta = today - given_date
        return delta.days

    except ValueError:
        
        return None
result = get_days_from_today("1854-10-05")
print(result)